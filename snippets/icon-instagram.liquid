{% doc %}
  @param color - {string}
  @param color_fill - {string}
  @param size - {string}

  @description Accept: color, color_fill coulb be empty, size
  @description Renders instagram icon

  @example {% render 'icon-instagram', color:"", color_fill:"", size:"" %}
{% enddoc %}

<svg
  xmlns="http://www.w3.org/2000/svg"
  viewBox="0 0 64 64"
  stroke-width="2.5"
  stroke="{%- if color != blank -%} {{- color -}} {%- else -%} #CACBD2 {%- endif -%}"
  fill="{%- if color_fill -%} {{- color_fill -}} {%- else -%} none {%- endif -%}"
  class="duration-300 transform transition-all"
  style="width: {{- size | default: 48 -}}px; height: {{- size | default: 48 -}}px;"
>
  <rect x="13.8" y="13.02" width="36.4" height="37.97" rx="10.53" stroke-linecap="round"></rect><circle cx="32" cy="32" r="9.28" stroke-linecap="round"></circle><circle cx="43.11" cy="19.74" r="1.32"></circle>
</svg>
