{% doc %}
  Renders icon user

  @param {string} color - The color of the icon
  @param {string} width - The width of the icon
  @param {string} height - The heigth of the icon

  @example
  {% render 'icon-menu', color: "#000000", width: "24px", height: "24px" %}
{% enddoc %}

<svg
  xmlns="http://www.w3.org/2000/svg"
  viewBox="0 0 64 64"
  stroke-width="2.5"
  stroke="{{ color }}"
  fill="none"
  class="duration-300 transform transition-all"
  style="width: {{ width}} ; height: {{ height }};"
>
